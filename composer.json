{
    "name": "dpominov/amo-crm",
    "description": "ООП клиент для работы с API amoCRM",
    "license": "MIT",
    "keywords": ["amocrm", "crm", "api"],
    "authors": [
        {
            "name": "dpominov",
            "email": "dpominov@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*",
        "dpominov/base-data-model":"1.0"
    },
    "autoload": {
        "psr-4": {
            "AmoCrm\\": "src"
        }
    },
    "repositories": {
        "base-data-model": {
            "type": "package",
            "package": {
                "name": "dpominov/base-data-model",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/dpominov/base-data-model.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "autoload": {
                    "psr-4": {
                        "BaseDataModel\\": "src"
                    }
                }
            }
        }
    }
}
